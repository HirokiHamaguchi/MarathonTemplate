    "\n",
    "\n",
    "def readFile(seed: int):\n",
    "    with open(f\"../../tools/in/{seed:04}.txt\") as f:\n",
    "        N, M = map(int, f.readline().split())\n",
    "    print(N, M)\n",
    "\n",
    "    plt.figure(figsize=(10, 10))\n",
    "    plt.title(f\"seed={seed}\")\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "readFile(0)"
